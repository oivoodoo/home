border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

border-shadow()
  -webkit-box-shadow arguments
  -moz-box-shadow arguments
  box-shadow arguments

control-rotate()
  -webkit-transform rotate(arguments)
  -moz-transform rotate(arguments)

html, body, div, span, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, font, img, ul, li
  margin 0
  padding 0
  border 0
  outline 0
  font-size 100%
  vertical-align baseline

body
  font 12px Georgia, Verdana, Lucida Sans Unicode, sans-serif
  line-height 1px
  color #999
  background url(/images/background.jpg) repeat
  h1
    font-size 50px
  p
    margin 15px 0

#page
  margin 0 auto
  padding 20px

#page_inner
  margin 0 auto
  width 820px
  #menu
    width 400px
    position relative
    top 10px
    left 20px
    z-index 80
    li
      control-rotate -45deg
      width 50px
      overflow hidden
      margin 10px 0px
      padding 5px 5px 5px 18px
      float left
      text-align right
    li.l1
      backgroud rgba(131, 178, 51, 0.65)
    li.l2
      backgroud rgba(196, 89, 30, 0.65)
    li.l3
      backgroud rgba(65, 117, 160, 0.65)
    li.l4
      backgroud rgba(65, 117, 160, 0.65)
  #container
    z-index 1000
    margin-top 30px
    padding 10px
    min-height 320px
    height 600px
    backgroud white
    border-radius 10px
    border-shadow 0px 0px 8px rgba(0, 0, 0, 0.296875)
